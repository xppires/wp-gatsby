{
  "name": "gatsby-starter-morning-dew",
  "version": "0.0.0-development",
  "description": "Starter Morning Dew Blog",
  "keywords": [
    "blog",
    "gatsby",
    "gatsby-plugin",
    "gatsby-theme",
    "starter"
  ],
  "homepage": "https://github.com/maxpou/gatsby-starter-morning-dew#readme",
  "bugs": {
    "url": "https://github.com/maxpou/gatsby-starter-morning-dew/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxpou/gatsby-starter-morning-dew.git"
  },
  "license": "MIT",
  "author": "Maxence Poutord <maxence.poutord@gmail.com>",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build && cmd /c sh ./scripts/urlsimples.sh ",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js' 'data/siteConfig.js' 'gatsby-*.js'",
    "generatePostPreviewImages": "node ./scripts/generatePostPreviewImages.js",
    "lh": "lighthousebot",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "semantic-release": "semantic-release",
    "start": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "s3dev": "cmd /c aws s3 sync public\\  s3://www.opassoapasso.com --exclude \"*posts/*\"  --exclude \"*tag/*\"  --exclude \"*category/*\" --acl public-read && aws s3  sync public\\posts  s3://www.opassoapasso.com/  --content-type text/html --acl public-read && aws s3  sync public\\tag  s3://www.opassoapasso.com/tag/  --content-type text/html --acl public-read&& aws s3  sync public\\category  s3://www.opassoapasso.com/category/  --content-type text/html --acl public-read&& npm run ss3tags ",
    "cleancloudfront": "cmd /c aws cloudfront create-invalidation --distribution-id E3LBC2AG3IA7F --paths \"/*\"",
    "ss3tags": "cmd /c sh  ./scripts/s3upload.sh && gatsby",
    "deploy": "gatsby clean && npm run build && npm run s3dev && npm run cleancloudfront "
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "@reach/router": "^1.3.3",
    "babel-plugin-styled-components": "^1.10.7",
    "disqus-react": "^1.0.8",
    "gatsby": "^2.20.12",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-compile-es6-packages": "^1.1.0",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-mdx": "^1.1.4",
    "gatsby-plugin-offline": "^2.2.1",
    "gatsby-plugin-page-creator": "^2.2.1",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-plugin-webfonts": "^1.1.2",
    "gatsby-remark-autolink-headers": "^2.2.1",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-images": "^3.2.2",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-responsive-iframe": "^2.3.1",
    "gatsby-remark-smartypants": "^2.2.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-source-mysql": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.4",
    "lodash": "^4.17.5",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-facebook": "^8.1.4",
    "react-helmet": "^5.2.1",
    "react-youtube": "^7.11.1",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.19.0",
    "lighthousebot": "git+https://github.com/GoogleChromeLabs/lighthousebot.git",
    "prettier": "^1.19.1",
    "puppeteer": "^1.17.0",
    "semantic-release": "^15.14.0",
    "yaml": "^1.8.3"
  },
  "peerDependencies": {
    "gatsby": "^2.13.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
